package main
import "fmt"

func main() {
    var n1, n2 int
    //Entrada
    fmt.Scanf("%f %f", &n1, n2)
    //Laço/Saída
    if n1 % 2 == 0 {
        for i:=2; i <= n2; i++ {
        fmt.Printf("%d ", n1)
        n1 += 2
        }
    } else {
    //Saída
        fmt.Println("O PRIMEIRO NÚMERO NÃO É PAR")
    }
}
