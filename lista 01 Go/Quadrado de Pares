package main
import"fmt"

func main() {
    var N , quadrado int
//ENTRADA
    fmt.Print("Insira um número inteiro entre 5 e 2000: ")
    fmt.Scan(&N)
//VERIFICAÇÃO
    if N < 5 || N > 2000{
        fmt.Print("NUMERO INVALIDO")
        return
    }
//LAÇO DE REPETIÇÃO
    for i:= 2; i <= N; i+= 2{
        quadrado= i*i
       fmt.Printf("%d^%d = %d\n", i, i, quadrado) 
    }
}
