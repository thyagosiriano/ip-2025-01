package main
import"fmt"

func main() {
    var salario, reajustado float64
    //Entrada
    fmt.Scan(&salario)
    //Cálculo
    if salario <= 300.00{
        reajustado= salario * 1.5
    } else{
        reajustado= salario * 1.3
    }
    //Saída
    fmt.Printf("SALARIO COM REAJUSTE = %.2f\n", reajustado)
}
