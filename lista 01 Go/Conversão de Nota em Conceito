package main
import "fmt"

func main() {
    var nota float64
    fmt.Scan(&nota)
    if nota >= 9.0{
        fmt.Printf("NOTA= %.1f CONCEITO= A\n", nota)
    }else if nota >= 7.5{
        fmt.Printf("NOTA= %.1f CONCEITO= B\n", nota)
    }else if nota >= 6.0{
        fmt.Printf("NOTA= %.1f CONCEITO= C\n", nota)
    }else{
        fmt.Printf("NOTA= %.1f CONCEITO= D\n", nota)
    }
}
