
package main
import(
    "fmt"
    "math"
)

func main() {
    var A, B, C, D, X1, X2 float64

    fmt.Scan(&A)
    fmt.Scan(&B)
    fmt.Scan(&C)
    
    if A<=0 {
        fmt.Print("Nao e equacao do segundo grau")
        return
    }else{
    D = B*B - 4*A*C
    }
    if D < 0{
        fmt.Println("Nao ha raizes reais")
        return
    }else{
        X1 = (-B + math.Sqrt(D))/(2*A)
        X2 = (-B - math.Sqrt(D))/(2*A)
    }
    fmt.Printf("%.5f %.5f", X1,  X2)
}
